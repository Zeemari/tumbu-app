{
  "expo": {
    "name": "zeekart",
    "slug": "zeekart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cart.jpg",
    "userInterfaceStyle": "light",
    "scheme": "https",
    "splash": {
      "image": "./assets/cart.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#5a5a5a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cart.jpg",
        "backgroundColor": "#5a5a5a"
      },
      "package": "com.drsaffas.zeekart",
      "permissions" : ["INTERNET"]
    },
    "web": {
      "favicon": "./assets/cart.jpg"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "22720e10-ecb8-4f6d-a6c1-db8eb07bcdf4"
      }
    }
  }
}
